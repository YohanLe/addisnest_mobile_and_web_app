# Addinest Real Estate Platform - Environment Variables
# Copy this file to .env and update the values with your actual configuration

# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Connection
MONGO_URI=mongodb://localhost:27017/addinest_real_estate
# For production, use your MongoDB Atlas or other provider connection string
# MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/addinest_real_estate

# JWT Authentication
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRE=30d
